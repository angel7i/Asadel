	<subDataset name="Listas" uuid="682caef2-0ea3-4853-823c-7ae4bebdfb2a">
		<queryString>
			<![CDATA[select Lista.idLista as Lista, Fecha, Lista.Hora, Lista.Total, CajaDia.Recaudado
from Lista join Cajadia on
Lista.CajaDia_idcajadia = cajadia.idcajadia and Lista.Estado = '0'
where cajadia.idcajadia = (select idcajadia from cajadia order by idcajadia desc limit 1)]]>
		</queryString>
