	<subDataset name="Articulo" uuid="682caef2-0ea3-4853-823c-7ae4bebdfb2a">
		<queryString>
			<![CDATA[select CajaDia.Estado, Lista.idLista as Lista, Lista.Hora, CajaDia.Recaudado as Total,
articulovendido.Nombre, articulovendido.Cantidad, articulovendido.Precio,
articulovendido.Subtotal from articulovendido join Lista on
articulovendido.Lista_idlista = Lista.idlista join Cajadia on
Lista.CajaDia_idcajadia = cajadia.idcajadia and Lista.Estado = '0'
where cajadia.idcajadia = (select idcajadia from cajadia order by idcajadia desc limit 1)]]>
		</queryString>
